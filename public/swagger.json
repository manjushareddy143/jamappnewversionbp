                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Enter email and new password for change current user password.",
                        "required": true,
                        "schema": {
                            "definition": "password_change",
