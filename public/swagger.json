                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Enter username, email address and password for user Register",
                        "required": true,
                        "schema": {
                            "definition": "users",
                            "required": [
                                "username",
                                "email",
                                "password"
                            ],
                            "properties": {
